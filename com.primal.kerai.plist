<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.primal.kerai</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/primal/apps/primal-host/kerai/target/release/kerai</string>
        <string>serve</string>
    </array>
    <key>EnvironmentVariables</key>
    <dict>
        <key>DATABASE_URL</key>
        <string>host=/tmp dbname=kerai</string>
    </dict>
    <key>UserName</key>
    <string>primal</string>
    <key>GroupName</key>
    <string>staff</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>ExitTimeOut</key>
    <integer>30</integer>
    <key>StandardOutPath</key>
    <string>/Users/primal/.kerai/serve.stdout.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/primal/.kerai/serve.stderr.log</string>
</dict>
</plist>
